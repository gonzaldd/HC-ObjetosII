<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.CategoriaDeConsulta" table="categoriadeconsulta">
		<id column="idCategoriaDeConsulta" name="idCategoriaDeConsulta">
			<generator class="identity"/>
		</id>
		<property column="nombreCategoriaDeConsulta" name="nombreCategoriaDeConsulta" type="string"/>
		<set name="consultas" table="consulta" inverse="true" lazy="true" fetch="select">
			<key>
				<column name="idConsulta" not-null="true" />
			</key>
			<one-to-many class="datos.Consulta" />
		</set>
	</class>
</hibernate-mapping>