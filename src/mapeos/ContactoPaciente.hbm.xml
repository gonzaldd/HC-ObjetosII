<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.ContactoPaciente" table="contactopaciente">
		<id name="idPaciente" column="idPaciente">         
    		<generator class="foreign">
				<param name="property">paciente</param>
		</generator>
		</id>
	
	<property column="telefonoPaciente" name="telefonoPaciente" type="string"/>
	<property column="direccionPaciente" name="direccionPaciente" type="string"/>
	<property column="mailPaciente" name="mailPaciente" type="string"/>
	<property column="fechaDeNacimientoPaciente" name="fechaDeNacimientoPaciente" type="calendar"/>
	<property column="nacionalidadPaciente" name="nacionalidadPaciente" type="string"/>
	
	<one-to-one name="paciente" class="datos.Paciente"/>
	</class>
</hibernate-mapping>